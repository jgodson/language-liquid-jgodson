{
	"name": "language-liquid-jgodson",
	"displayName": "Liquid Languages Support",
	"description": "Liquid Languages Syntax Highlighter",
	"version": "0.0.3",
	"publisher": "jgodson",
	"engines": {
		"vscode": "^1.0.0"
	},
	"categories": [
		"Languages"
	],
	"contributes": {
		"languages": [
			{
				"id": "liquid",
				"aliases": [
					"HTML Liquid",
					"liquid"
				],
				"extensions": [
					".liquid"
				],
				"configuration": "./liquid.configuration.json"
			},
			{
				"id": "liquid scss",
				"aliases": [
					"SCSS Liquid",
					"liquid"
				],
				"extensions": [
					"scss.liquid",
					"css.scss.liquid"
				],
				"configuration": "./liquid.configuration.json"
			},
			{
				"id": "liquid javascript",
				"aliases": [
					"JavaScript Liquid",
					"liquid"
				],
				"extensions": [
					"js.liquid"
				],
				"configuration": "./liquid.configuration.json"
			},
			{
				"id": "liquid css",
				"aliases": [
					"CSS Liquid",
					"liquid"
				],
				"extensions": [
					"css.liquid"
				],
				"configuration": "./liquid.configuration.json"
			}
		],
		"grammars": [
			{
				"language": "liquid",
				"scopeName": "text.html.liquid",
				"path": "./syntaxes/liquid.tmLanguage"
			},
			{
				"language": "liquid scss",
				"scopeName": "text.scss.liquid",
				"path": "./syntaxes/liquid.scss.tmLanguage"
			},
			{
				"language": "liquid javascript",
				"scopeName": "text.js.liquid",
				"path": "./syntaxes/liquid.js.tmLanguage"
			},
			{
				"language": "liquid css",
				"scopeName": "text.css.liquid",
				"path": "./syntaxes/liquid.css.tmLanguage"
			}
		]
	}
}